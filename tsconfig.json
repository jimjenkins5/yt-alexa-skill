{
   "compilerOptions": {
      "lib": [
         "es2018"
      ],
      "moduleResolution": "node",
      "esModuleInterop": true,
      "noImplicitAny": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "sourceMap": true,
      "allowJs": true,
      "target": "es2017",
      "baseUrl": ".",
      "outDir": "dist"
   },
   "exclude": [
      "node_modules"
   ]
}
