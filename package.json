{
  "name": "serverless-typescript-starter",
  "version": "1.0.0",
  "description": "Serverless webpack example using Typescript",
  "main": "handler.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint '**/*.{js,ts}'",
    "lint:fix": "eslint '**/*.{js,ts}' --fix"
  },
  "dependencies": {
    "ask-sdk": "2.4.0",
    "mysql2": "1.6.5",
    "reflect-metadata": "0.1.13",
    "sequelize": "4.43.0",
    "sequelize-typescript": "0.6.7",
    "tedious": "1.7.0"
  },
  "devDependencies": {
    "@commitlint/travis-cli": "7.5.2",
    "@creativepenguin/standards": "0.1.0",
    "@types/aws-lambda": "8.10.21",
    "@types/node": "8.10.43",
    "@types/sequelize": "4.27.39",
    "config": "3.0.1",
    "eslint": "5.15.1",
    "husky": "1.3.1",
    "serverless-webpack": "5.2.0",
    "source-map-support": "0.5.10",
    "ts-loader": "5.3.3",
    "typescript": "3.3.3333",
    "webpack": "4.29.6"
  },
  "author": "Jim Jenkins <jimjenkins5@gmail.com>",
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/@creativepenguin/standards/commitizen.js"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
